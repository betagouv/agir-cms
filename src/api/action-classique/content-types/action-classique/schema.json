{
  "kind": "collectionType",
  "collectionName": "action_classiques",
  "info": {
    "singularName": "action-classique",
    "pluralName": "action-classiques",
    "displayName": "Action Classique",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "regex": "^[a-z_]+$",
      "unique": true,
      "required": true,
      "minLength": 4
    },
    "sous_titre": {
      "type": "string"
    },
    "pourquoi": {
      "type": "text",
      "required": true
    },
    "comment": {
      "type": "text",
      "required": true
    },
    "thematique": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::thematique.thematique",
      "inversedBy": "action_classiques"
    },
    "besoins": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::besoin.besoin",
      "inversedBy": "action_classiques"
    },
    "categorie_recettes": {
      "type": "enumeration",
      "enum": [
        "vegan",
        "vege",
        "dinde_volaille",
        "saison",
        "poisson"
      ]
    },
    "action_lvo": {
      "type": "enumeration",
      "enum": [
        "donner",
        "reparer",
        "vendre",
        "louer",
        "acheter",
        "emprunter"
      ]
    },
    "objet_lvo": {
      "type": "enumeration",
      "enum": [
        "chaussure",
        "smartphone",
        "chaise",
        "portable",
        "vélo",
        "voiture"
      ]
    },
    "faqs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faq.faq",
      "mappedBy": "action_classiques"
    },
    "ngc_action_rule": {
      "type": "string"
    },
    "consigne": {
      "type": "string",
      "default": "Réalisez cette action dans les prochaines semaines et partagez vos retours",
      "required": true
    },
    "label_compteur": {
      "type": "string",
      "default": "**{NBR_ACTIONS} actions** réalisées par la communauté",
      "required": true
    },
    "sources": {
      "type": "component",
      "repeatable": true,
      "component": "sources.source"
    },
    "categorie_pdcn": {
      "type": "enumeration",
      "enum": [
        "nourriture",
        "circuit_court",
        "epicerie_superette",
        "marche_local",
        "zero_dechet"
      ]
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "action_classiques"
    },
    "tag_v2_incluants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag-v2.tag-v2",
      "inversedBy": "action_classiques_inclues"
    },
    "tag_v2_excluants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag-v2.tag-v2",
      "inversedBy": "action_classiques_exclues"
    },
    "VISIBLE_PROD": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "sous_thematique": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sous-thematique.sous-thematique",
      "inversedBy": "action_classiques"
    },
    "emoji": {
      "type": "string",
      "required": false,
      "maxLength": 1,
      "minLength": 1
    }
  }
}
