{
  "kind": "collectionType",
  "collectionName": "action_quizzes",
  "info": {
    "singularName": "action-quizz",
    "pluralName": "action-quizzes",
    "displayName": "Action Quizz",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "sous_titre": {
      "type": "string"
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 4,
      "regex": "^[a-z_]+$"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quizz.quizz",
      "inversedBy": "action_quizzes"
    },
    "thematique": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::thematique.thematique",
      "inversedBy": "action_quizzes"
    },
    "felicitations": {
      "type": "string",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "action_quizzes"
    },
    "tag_v2_incluants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag-v2.tag-v2",
      "inversedBy": "action_quizzes_inclues"
    },
    "tag_v2_excluants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag-v2.tag-v2",
      "inversedBy": "action_quizzes_exclues"
    },
    "VISIBLE_PROD": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "emoji": {
      "type": "string",
      "minLength": 1,
      "maxLength": 2
    },
    "sous_thematique": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sous-thematique.sous-thematique",
      "inversedBy": "action_quizzes"
    }
  }
}
