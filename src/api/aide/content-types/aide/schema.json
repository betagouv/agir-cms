{
  "kind": "collectionType",
  "collectionName": "aides",
  "info": {
    "singularName": "aide",
    "pluralName": "aides",
    "displayName": "Aide",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "titre"
    }
  },
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "echelle": {
      "type": "enumeration",
      "enum": [
        "National",
        "Région",
        "Département",
        "Métropole",
        "Agglomération",
        "Commune",
        "Communauté de communes",
        "Communauté urbaine",
        "Communauté d'agglomération"
      ]
    },
    "url_source": {
      "type": "string"
    },
    "codes_postaux": {
      "type": "text"
    },
    "thematiques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::thematique.thematique",
      "mappedBy": "aides"
    },
    "rubriques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::rubrique.rubrique",
      "mappedBy": "aides"
    },
    "is_simulation": {
      "type": "boolean"
    },
    "url_detail_front": {
      "type": "string"
    },
    "montantMaximum": {
      "type": "string"
    },
    "besoin": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::besoin.besoin",
      "inversedBy": "aides"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "aides"
    },
    "include_codes_commune": {
      "type": "text"
    },
    "exclude_codes_commune": {
      "type": "text"
    },
    "codes_departement": {
      "type": "text"
    },
    "codes_region": {
      "type": "text"
    }
  }
}
