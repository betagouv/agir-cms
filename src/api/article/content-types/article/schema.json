{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "sousTitre": {
      "type": "string"
    },
    "imageUrl": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contenu": {
      "type": "richtext",
      "required": true
    },
    "source": {
      "type": "string"
    },
    "partenaire": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::partenaire.partenaire",
      "inversedBy": "articles"
    },
    "niveau": {
      "type": "component",
      "repeatable": false,
      "component": "niveau.difficulte"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quizz.quizz",
      "inversedBy": "articles"
    },
    "saison": {
      "type": "component",
      "repeatable": false,
      "component": "periode.saison"
    },
    "codePostal": {
      "type": "string"
    },
    "duree": {
      "type": "string"
    },
    "frequence": {
      "type": "string"
    },
    "points": {
      "type": "integer",
      "min": 0,
      "max": 1000
    },
    "difficulty": {
      "type": "integer",
      "max": 5,
      "min": 1,
      "default": 1,
      "required": false
    }
  }
}
