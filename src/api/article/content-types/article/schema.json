{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries": {
      "idField": "titre"
    }
  },
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "sousTitre": {
      "type": "string"
    },
    "imageUrl": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contenu": {
      "type": "richtext",
      "required": true
    },
    "source": {
      "type": "string"
    },
    "partenaire": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::partenaire.partenaire",
      "inversedBy": "articles"
    },
    "quizzes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::quizz.quizz",
      "inversedBy": "articles"
    },
    "codes_postaux": {
      "type": "text"
    },
    "duree": {
      "type": "string"
    },
    "frequence": {
      "type": "string"
    },
    "points": {
      "type": "integer",
      "min": 0,
      "max": 1000
    },
    "difficulty": {
      "type": "integer",
      "max": 5,
      "min": 1,
      "default": 1,
      "required": false
    },
    "thematiques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::thematique.thematique",
      "mappedBy": "articles"
    },
    "rubriques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::rubrique.rubrique",
      "mappedBy": "articles"
    },
    "thematique_gamification": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::thematique.thematique",
      "inversedBy": "articles_gamifications"
    },
    "sources": {
      "type": "component",
      "repeatable": true,
      "component": "sources.source"
    },
    "categorie": {
      "type": "enumeration",
      "enum": [
        "recommandation",
        "mission",
        "test"
      ],
      "required": true,
      "default": "recommandation"
    },
    "mois": {
      "type": "string"
    },
    "include_codes_commune": {
      "type": "text"
    },
    "exclude_codes_commune": {
      "type": "text"
    },
    "codes_departement": {
      "type": "text"
    },
    "codes_region": {
      "type": "text"
    },
    "tag_article": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tag-article.tag-article",
      "inversedBy": "articles"
    },
    "echelle": {
      "type": "enumeration",
      "enum": [
        "National",
        "Région",
        "Département",
        "Métropole",
        "Agglomération",
        "Commune",
        "Communauté de communes",
        "Communauté urbaine",
        "Communauté d'agglomération"
      ]
    },
    "derniere_maj": {
      "type": "date"
    }
  }
}
