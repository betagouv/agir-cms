{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quizz",
    "pluralName": "quizzes",
    "displayName": "Quizz",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "questions": {
      "type": "component",
      "repeatable": true,
      "component": "quizz.question"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "quizzes"
    },
    "duree": {
      "type": "string",
      "required": true
    },
    "points": {
      "type": "integer",
      "max": 1000,
      "min": 0,
      "required": true
    },
    "sousTitre": {
      "type": "string"
    },
    "imageUrl": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "frequence": {
      "type": "string"
    },
    "difficulty": {
      "type": "integer",
      "max": 5,
      "min": 1,
      "default": 1
    },
    "thematiques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::thematique.thematique",
      "mappedBy": "quizzes",
      "required": true
    },
    "thematique_gamification": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::thematique.thematique",
      "inversedBy": "quizzes_gamification",
      "required": true
    },
    "rubriques": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::rubrique.rubrique",
      "mappedBy": "quizzes"
    },
    "partenaire": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::partenaire.partenaire"
    }
  }
}
