{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quizz",
    "pluralName": "quizzes",
    "displayName": "Quizz",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string"
    },
    "niveau": {
      "type": "component",
      "repeatable": false,
      "component": "niveau.difficulte"
    },
    "questions": {
      "type": "component",
      "repeatable": true,
      "component": "quizz.question"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "quizzes"
    },
    "sujets": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categorie.categorie",
      "inversedBy": "quizzes"
    },
    "duree": {
      "type": "string"
    },
    "points": {
      "type": "integer",
      "max": 1000,
      "min": 0
    },
    "sousTitre": {
      "type": "string"
    },
    "imageUrl": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
