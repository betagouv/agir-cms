{
  "kind": "collectionType",
  "collectionName": "kycs",
  "info": {
    "singularName": "kyc",
    "pluralName": "kycs",
    "displayName": "KYC",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": ["libre", "choix_unique", "choix_multiple", "entier", "decimal"],
      "required": true
    },
    "categorie": {
      "type": "enumeration",
      "enum": ["mission", "recommandation", "test"],
      "required": true,
      "default": "recommandation"
    },
    "points": {
      "type": "integer",
      "required": true
    },
    "is_ngc": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "reponses": {
      "type": "component",
      "repeatable": true,
      "component": "kyc.reponses",
      "required": false
    },
    "thematique": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::thematique.thematique",
      "inversedBy": "kycs"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "kycs"
    },
    "ngc_key": {
      "type": "string"
    },
    "short_question": {
      "type": "string"
    },
    "imageUrl": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "OR_Conditions": {
      "type": "component",
      "repeatable": true,
      "component": "defi.non"
    },
    "unite": {
      "type": "enumeration",
      "enum": [
        "kg (kilogramme)",
        "g (gramme)",
        "km (kilomètre)",
        "l (litre)",
        "€ (euro)",
        "€/an (euro par an)",
        "an",
        "l/100km (litre au cent)",
        "€/l (euro par litre)",
        "kWh (kilowattheure)",
        "€/kWh (euro par kilowattheure)",
        "kWh/100km (kilowattheure au cent)"
      ]
    },
    "emoji": {
      "type": "string"
    },
    "A_SUPPRIMER": {
      "type": "boolean"
    },
    "action_bilans": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::action-bilan.action-bilan",
      "mappedBy": "kycs"
    },
    "action_simulateurs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::action-simulateur.action-simulateur",
      "mappedBy": "kycs"
    }
  }
}
