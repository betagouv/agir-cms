{
  "kind": "collectionType",
  "collectionName": "kycs",
  "info": {
    "singularName": "kyc",
    "pluralName": "kycs",
    "displayName": "KYC",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "libre",
        "choix_unique",
        "choix_multiple",
        "entier",
        "decimal"
      ],
      "required": true
    },
    "categorie": {
      "type": "enumeration",
      "enum": [
        "mission",
        "recommandation",
        "test"
      ],
      "required": true,
      "default": "recommandation"
    },
    "points": {
      "type": "integer",
      "required": true
    },
    "is_ngc": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "reponses": {
      "type": "component",
      "repeatable": true,
      "component": "kyc.reponses",
      "required": false
    },
    "thematique": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::thematique.thematique",
      "inversedBy": "kycs"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "kycs"
    },
    "univers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::univers.univers",
      "inversedBy": "kycs"
    },
    "ngc_key": {
      "type": "string"
    },
    "short_question": {
      "type": "string"
    },
    "imageUrl": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "OR_Conditions": {
      "type": "component",
      "repeatable": true,
      "component": "defi.non"
    },
    "unite": {
      "type": "enumeration",
      "enum": [
        "kg (kilogramme)",
        "g (gramme)",
        "km (kilom√®tre)",
        "l (litre)",
        "euro (euro)"
      ]
    },
    "emoji": {
      "type": "string"
    }
  }
}
